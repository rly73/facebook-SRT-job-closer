{
  "manifest_version": 2,
  "name": "Closing Script Test",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "/resources/images/icon16.png",
    "32": "/resources/images/icon48.png" ,
    "128": "/resources/images/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_icon": "/resources/images/icon48.png",
    "default_popup": "popup.html",
    "default_title": "A popup will come here"
  }
}
